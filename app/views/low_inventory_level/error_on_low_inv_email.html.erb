<h3>Low inventory email sending failed:</h3>
<% unless @exception.nil? %>
  <p><strong>Tenant Name: </strong><%=@tenant%></p>
  <p><strong>Error Date/Time: </strong><%=(Time.now + GeneralSetting.first.time_zone.to_i).strftime("%e %b %Y %H:%M")%></p>
  <% unless @exception.nil? %>
  <p><strong>Exception message: </strong><%=@exception.message%></p>
  <p><strong>Exception backtrace: </strong></p>
  <p><%= raw(@exception.backtrace.join("<br/>")) rescue "" %></p>
  <% end %>
<%end%>

