<div style="margin-left: 250px; padding-top: 20px;">
	<div class="well span6">
		<div class="span1">
			<%= link_to 'Back', {action: 'confirmation', controller: 'orders'}, class: "btn btn-primary"%>
		</div>
		<div>
			<table border='1' style="border: 1px solid #98bf21; margin-top: 5px;" class="span5">
				<thead style="background-color: #A7C942; text-align: center;">
					<td>Order Number</td>
					<td>Tracking Number</td>
				</thead>
				<% @orders.each do |order| %>
				  <tr>
				  	<td><%= order.increment_id%></td>
				  	<td><%= link_to order.tracking_num, "https://www.google.com/search?q=" + order.tracking_num%></td>
				<% end %>
			</table>
		</div>
	</div>
</div>