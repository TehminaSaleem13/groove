<div class="row">
  <div class="box-outer">
    <div class="box">
      <%= render 'shared/existing_customer', shop: @shop_name %>
    </div>
    <div class="col-sm-offset-3 col-sm-6">
      <img src="/assets/images/logo.png" class="col-xs-12" alt="GroovePacker"/>
    </div>
  </div>
</div>
