    </div>
    <div class="col-box">
      2 Users<br/> <i class="fa fa-user" style="font-size: 32px; margin-top: 5px;"></i>
    </div>
    <br/>

    <div class="text-center">
      <%= link_to 'Get Started', {action: 'new', controller: 'subscriptions', plan_id: 'groove-duo-60', shop_name: params[:shop_name], shop_type: params[:shop_type], email: params[:email]}, class: "btn btn-success" %>
    </div>
  </div>

